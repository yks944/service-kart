<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
  <servlet>
  	<servlet-name>UpdateAddress</servlet-name>
  	<display-name>UpdateAddress</display-name>
  	<description></description>
  	<servlet-class>Address.UpdateAddress</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SessionAdd</servlet-name>
  	<display-name>SessionAdd</display-name>
  	<description></description>
  	<servlet-class>Session.SessionAdd</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SmsServlet</servlet-name>
  	<display-name>SmsServlet</display-name>
  	<description></description>
  	<servlet-class>sms.SmsServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetDetailsController</servlet-name>
  	<display-name>GetDetailsController</display-name>
  	<description></description>
  	<servlet-class>Controller.GetDetailsController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SessionLogout</servlet-name>
  	<display-name>SessionLogout</display-name>
  	<description></description>
  	<servlet-class>Session.SessionLogout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UpdateAddress</servlet-name>
  	<url-pattern>/UpdateAddress</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SessionAdd</servlet-name>
  	<url-pattern>/SessionAdd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SmsServlet</servlet-name>
  	<url-pattern>/SmsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetDetailsController</servlet-name>
  	<url-pattern>/GetDetailsController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SessionLogout</servlet-name>
  	<url-pattern>/SessionLogout</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/Error404.jsp</location>
</error-page>

</web-app>
